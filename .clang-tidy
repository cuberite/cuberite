#  value:           CamelCase
#- key:             readability-identifier-naming.FunctionCase
#  value:           CamelCase
#- key:             readability-identifier-naming.EnumCase
#  value:           camelBack
WarningsAsErrors: '*'
HeaderFilterRegex: '/cuberite/src/\.?[^\.]'
